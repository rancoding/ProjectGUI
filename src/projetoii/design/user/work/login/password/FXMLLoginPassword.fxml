<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="320.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projetoii.design.user.work.login.password.FXMLLoginPasswordController">
    <children>
      <PasswordField fx:id="display" editable="false" layoutX="10.0" layoutY="10.0" prefHeight="60.0" prefWidth="260.0" />
      <Button fx:id="button0" layoutX="10.0" layoutY="260.0" mnemonicParsing="false" onAction="#onNumberClick" prefHeight="50.0" prefWidth="80.0" text="0" />
      <Button fx:id="button1" layoutX="10.0" layoutY="200.0" mnemonicParsing="false" onAction="#onNumberClick" prefHeight="50.0" prefWidth="80.0" text="1" />
      <Button fx:id="button2" layoutX="100.0" layoutY="200.0" mnemonicParsing="false" onAction="#onNumberClick" prefHeight="50.0" prefWidth="80.0" text="2" />
      <Button fx:id="button3" layoutX="190.0" layoutY="200.0" mnemonicParsing="false" onAction="#onNumberClick" prefHeight="50.0" prefWidth="80.0" text="3" />
      <Button fx:id="button4" layoutX="10.0" layoutY="140.0" mnemonicParsing="false" onAction="#onNumberClick" prefHeight="50.0" prefWidth="80.0" text="4" />
      <Button fx:id="button5" layoutX="100.0" layoutY="140.0" mnemonicParsing="false" onAction="#onNumberClick" prefHeight="50.0" prefWidth="80.0" text="5" />
      <Button fx:id="button6" layoutX="190.0" layoutY="140.0" mnemonicParsing="false" onAction="#onNumberClick" prefHeight="50.0" prefWidth="80.0" text="6" />
      <Button fx:id="button7" layoutX="10.0" layoutY="80.0" mnemonicParsing="false" onAction="#onNumberClick" prefHeight="50.0" prefWidth="80.0" text="7" />
      <Button fx:id="button8" layoutX="100.0" layoutY="80.0" mnemonicParsing="false" onAction="#onNumberClick" prefHeight="50.0" prefWidth="80.0" text="8" />
      <Button fx:id="button9" layoutX="190.0" layoutY="80.0" mnemonicParsing="false" onAction="#onNumberClick" prefHeight="50.0" prefWidth="80.0" text="9" />
      <Button fx:id="accept" layoutX="190.0" layoutY="260.0" mnemonicParsing="false" onAction="#onAcceptClick" prefHeight="50.0" prefWidth="80.0">
         <graphic>
            <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@image/accept.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="clear" layoutX="100.0" layoutY="260.0" mnemonicParsing="false" onAction="#onClearClick" prefHeight="50.0" prefWidth="80.0">
         <graphic>
            <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@image/clear.png" />
               </image>
            </ImageView>
         </graphic></Button>
   </children>
</AnchorPane>
